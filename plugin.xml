<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.myIdeaWay.plugin.bigbugIm" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>BigBugIM</name>
    <preference name="APP_KEY" />
    <js-module name="BigBugIM" src="www/BigBugIM.js">
        <clobbers target="cordova.plugins.BigBugIM"/>
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="BigBugIM">
                <param name="android-package" value="com.myIdeaWay.plugin.bigbugIm"/>
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml">
